{
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "yiisoft/yii2": "^2.0.47",
    "paulzi/yii2-json-behavior": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\modules\\poll\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "app\\modules\\poll\\tests\\": "tests"
    }
  }
}
